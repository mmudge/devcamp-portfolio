<main role="main" class="inner cover">
  <h1 class="cover-heading card-margin" id="tech-news-title">Developer News</h1>
  <h3><em>Recent Tweets</em></h3>

  <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">
            <%= twitter_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</main>
